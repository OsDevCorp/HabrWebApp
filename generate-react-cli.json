  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/default/TemplateName.tsx",
        "style": "templates/default/TemplateName.css",
        "index": "templates/default/index.ts",
        "story": "templates/default/TemplateName.stories.tsx",
        "test": "templates/default/TemplateName.test.tsx"
